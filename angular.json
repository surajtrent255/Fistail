{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "frontfishtail": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/frontfishtail",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css",
              "src/assets/css/bootstrap.min.css",
              "src/assets/css/line-icons.css",
              "src/assets/css/owl.carousel.css",
              "src/assets/css/owl.theme.css",
              "src/assets/css/nivo-lightbox.css",
              "src/assets/css/magnific-popup.css",
              "src/assets/css/animate.css",
              "src/assets/css/color-switcher.css",
              "src/assets/css/menu_sideslide.css",
              "src/assets/css/main.css",
              "src/assets/css/responsive.css"
            ],
            "scripts": [
              "src/assets/js/jquery-min.js",
              "src/assets/js/popper.min.js",
              "src/assets/js/bootstrap.min.js",
              "src/assets/js/classie.js",
              "src/assets/js/color-switcher.js",
              "src/assets/js/jquery.mixitup.js",
              "src/assets/js/nivo-lightbox.js",
              "src/assets/js/owl.carousel.js",
              "src/assets/js/jquery.stellar.min.js",
              "src/assets/js/jquery.nav.js",
              "src/assets/js/scrolling-nav.js",
              "src/assets/js/jquery.easing.min.js",
              "src/assets/js/wow.js",
              "src/assets/js/jquery.vide.js",
              "src/assets/js/jquery.counterup.min.js",
              "src/assets/js/jquery.magnific-popup.min.js",
              "src/assets/js/waypoints.min.js",
              "src/assets/js/form-validator.min.js",
              "src/assets/js/contact-form-script.js",
              "src/assets/js/main.js",
              "src/assets/js/map.js",
              "src/assets/js/menu.js"
              
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "4mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2mb",
                  "maximumError": "4mb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "frontfishtail:build:production"
            },
            "development": {
              "browserTarget": "frontfishtail:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "frontfishtail:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "d54b551e-4738-4339-8abd-4c45baf2ece8"
  }
}